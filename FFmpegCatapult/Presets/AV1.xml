<presets>
	<preset name="AV1, Standard Definition">
		<format>matroska</format>
		<vcodec>av1</vcodec>
		<quality>40</quality>
		<tilecolumns>1</tilecolumns>
		<tilerows>0</tilerows>
		<acodec>vorbis</acodec>
		<scale>1</scale>
		<width>0</width>
		<height>480</height>
	</preset>
	<preset name="AV1, High Definition">
		<format>matroska</format>
		<vcodec>av1</vcodec>
		<quality>30</quality>
		<tilecolumns>1</tilecolumns>
		<tilerows>0</tilerows>
		<acodec>vorbis</acodec>
		<scale>0</scale>
		<width>0</width>
		<height>1080</height>
	</preset>
</presets>