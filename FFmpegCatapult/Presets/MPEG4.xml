<presets>
	<preset name="DivX">
		<format>avi</format>
		<vcodec>mpeg4</vcodec>
		<vb>1000</vb>
		<maxvb>1500</maxvb>
		<buffersize>2048</buffersize>
		<bframes>2</bframes>
		<bfstrategy>2</bfstrategy>
		<trellis>2</trellis>
		<gopsize>300</gopsize>
		<acodec>mp3</acodec>
		<ab>128</ab>
		<samplerate>44100</samplerate>
		<channels>2</channels>
		<scale>1</scale>
		<width>512</width>
		<height>0</height>
		<ratio>16:9</ratio>
	</preset>
	<preset name="MPEG-4, Standard Definition">
		<format>mp4</format>
		<faststart>true</faststart>
		<vcodec>mpeg4</vcodec>
		<vb>2500</vb>
		<maxvb>4000</maxvb>
		<acodec>aac</acodec>
		<ab>96</ab>
		<scale>1</scale>
		<width>0</width>
		<height>480</height>
		<ratio>16:9</ratio>
	</preset>
	<preset name="MPEG-4, High Definition">
		<format>mp4</format>
		<faststart>false</faststart>
		<vcodec>mpeg4</vcodec>
		<vb>15000</vb>
		<maxvb>20000</maxvb>
		<acodec>aac</acodec>
		<ab>160</ab>
		<scale>0</scale>
		<width>0</width>
		<height>1080</height>
		<ratio>16:9</ratio>
	</preset>    
</presets>