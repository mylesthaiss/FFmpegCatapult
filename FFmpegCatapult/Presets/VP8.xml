<presets>
	<preset name="VP8, Standard Definition">
		<format>webm</format>
		<vcodec>vp8</vcodec>
		<vb>1000</vb>
		<maxvb>1500</maxvb>
		<qmin>0</qmin>
		<qmax>63</qmax>
		<acodec>vorbis</acodec>
		<scale>1</scale>
		<width>0</width>
		<height>480</height>
	</preset>
	<preset name="VP8, High Definition">
		<format>webm</format>
		<vcodec>vp8</vcodec>
		<vb>12000</vb>
		<maxvb>15000</maxvb>
		<qmin>10</qmin>
		<qmax>52</qmax>
		<acodec>vorbis</acodec>
		<scale>0</scale>
		<width>0</width>
		<height>1080</height>
	</preset>
</presets>