<presets>
	<preset name="H.264, Standard Definition">
		<format>mp4</format>
		<faststart>true</faststart>
		<vcodec>h264</vcodec>
		<vencoder>libx264</vencoder>
		<vpreset>medium</vpreset>
		<vprofile>main</vprofile>
		<level>3.1</level>
		<vb>3000</vb>
		<quality>18</quality>
		<acodec>aac</acodec>
		<ab>128</ab>
		<scale>1</scale>
		<width>0</width>
		<height>480</height>
		<ratio>16:9</ratio>
	</preset>
	<preset name="H.264, High Definition">
		<format>mp4</format>
		<faststart>false</faststart>
		<vcodec>h264</vcodec>
		<vencoder>libx264</vencoder>
		<vpreset>medium</vpreset>
		<vprofile>high</vprofile>
		<level>4.1</level>
		<quality>18</quality>
		<acodec>aac</acodec>
		<ab>192</ab>
		<scale>0</scale>
		<width>0</width>
		<height>1080</height>
		<ratio>16:9</ratio>
	</preset>    
</presets>